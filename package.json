{
  "name": "@dnlup/doc",
  "version": "1.2.0-0",
  "description": "See how many Gigawatts your Node.js process is using",
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "lint": "npm run lint:js && npm run lint:ts",
    "lint:js": "standard --fix",
    "lint:ts": "ts-standard --fix *d.ts",
    "test": "npm run test:js && npm run test:ts",
    "test:js": "tap test.js --lines 95 --branches 80 --statements 95",
    "test:ts": "tsd",
    "doc": "markdown-toc -i README.md",
    "release": "HUSKY_SKIP_HOOKS=1 standard-version",
    "postrelease": "npm run push && npm publish",
    "next": "HUSKY_SKIP_HOOKS=1 standard-version --prerelease",
    "postnext": "npm run push && npm publish --tag next",
    "push": "git push origin --follow-tags `git rev-parse --abbrev-ref HEAD`"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dnlup/doc.git"
  },
  "files": [
    "gc.js",
    "index.js",
    "index.d.ts"
  ],
  "keywords": [
    "process",
    "data",
    "memory",
    "cpu",
    "event",
    "loop",
    "delay",
    "health",
    "metrics"
  ],
  "author": "Daniele Belardi <dwon.dnl@gmail.com>",
  "license": "ISC",
  "publishConfig": {
    "access": "public"
  },
  "bugs": {
    "url": "https://github.com/dnlup/doc/issues"
  },
  "homepage": "https://github.com/dnlup/doc#readme",
  "engines": {
    "node": ">=10",
    "npm": ">=6"
  },
  "ts-standard": {
    "project": "./tsconfig.eslint.json"
  },
  "tsd": {
    "directory": "."
  },
  "devDependencies": {
    "@types/node": "^14.0.18",
    "husky": "^4.2.5",
    "is-ci": "^2.0.0",
    "lint-staged": "^10.2.10",
    "markdown-toc": "^1.2.0",
    "semver": "^7.3.2",
    "standard": "^14.3.4",
    "standard-version": "^8.0.0",
    "tap": "^14.10.7",
    "ts-standard": "^8.0.1",
    "tsd": "^0.13.1"
  }
}
